{
  "name": "toasted-notes",
  "version": "2.1.2",
  "description": "Flexible, easy to implement toast notifications for react",
  "main": "commonjs/index.js",
  "module": "lib/index.js",
  "typings": "lib/index.d.ts",
  "style": "src/styles.css",
  "scripts": {
    "build-library-commonjs": "rimraf ./commonjs && tsc -p tsconfig.lib.json --module commonjs --outDir commonjs",
    "build-library": "rimraf ./lib && tsc -p tsconfig.lib.json --outDir ./lib",
    "prepublish": "yarn run build-library && yarn run build-library-commonjs"
  },
  "dependencies": {
    "@reach/alert": "^0.1.2",
    "@types/react": "^16.8.10",
    "@types/react-dom": "^16.8.3",
    "react-spring": "^8.0.7"
  },
  "peerDependencies": {
    "react": "^16.8.4",
    "react-dom": "^16.8.4"
  },
  "devDependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^2.1.5",
    "rimraf": "^2.6.3",
    "typescript": "^3.3.4000"
  },
  "author": "Ben McMahen <ben.mcmahen@gmail.com>",
  "homepage": "https://github.com/bmcmahen/toasted-notes",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component",
    "toast",
    "notifications"
  ],
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
