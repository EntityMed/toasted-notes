    "@types/react": "^16.8.4",
    "@types/react-dom": "^16.8.2",
    "gh-pages": "^1.2.0",
    "prismjs": "^1.15.0",
    "react": "16.8.2",
    "react-dom": "16.8.2",
    "react-scripts": "^2.1.5",
    "rimraf": "^2.6.3",
    "rollup": "^1.2.2",
    "rollup-plugin-postcss": "^2.0.3",
    "rollup-plugin-typescript2": "^0.19.2",
